{
  "$id": "https://meta-mart.org/schema/auth/passwordResetRequest.json",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "PasswordResetRequest",
  "description": "This schema defines Email Verification Token Schema.",
  "type": "object",
  "javaType": "org.metamart.schema.auth.PasswordResetRequest",
  "properties": {
    "username": {
      "description": "UserName",
      "type": "string"
    },
    "password": {
      "description": "Password",
      "type": "string",
      "minLength": 8,
      "maxLength": 56
    },
    "confirmPassword": {
      "description": "Confirm Password",
      "type": "string",
      "minLength": 8,
      "maxLength": 56
    },
    "token": {
      "description": "Token",
      "type": "string"
    }
  },
  "additionalProperties": false,
  "required": [
    "username"
  ]
}